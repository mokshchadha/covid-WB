{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jasmine": true,
    "jquery": true
  },
  "extends": ["eslint:recommended","plugin:react/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "h": "readonly",
    "_": "readonly",
    "moment": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-empty": "warn",
    "no-prototype-builtins": "off",
    "no-useless-escape": "off",
    "react/prop-types":"off",
    "react/jsx-no-target-blank":"warn",
    "react/display-name": "off"
  }
}
